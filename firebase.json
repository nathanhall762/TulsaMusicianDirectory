{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/robots.txt",
        "destination": "/robots.txt"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "headers": [ {
  }, {
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=604800"
    } ]
  }]
}
