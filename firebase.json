{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "tmdserver",
          "region": "us-central1"
        }
      },
      {
        "source": "/robots.txt",
        "destination": "/robots.txt"
      }
    ]
  },
  "headers": [ {
  }, {
    "headers": [ {
      "key": "Cache-Control",
      "value": "max-age=604800"
    } ]
  }]
}
