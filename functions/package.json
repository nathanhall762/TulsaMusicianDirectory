    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "local": "npm run build && node ./lib/local"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^4.4.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "firebase-functions-test": "^3.1.0",
    "firebase-tools": "^12.6.1",
    "typescript": "^4.9.0"
  },
  "private": true
}
