{
  "name": "tulsamusiciandirectory",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=development node server/server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "tsc && vite build --outDir dist/client --ssrManifest",
    "build:server": "tsc && vite build --outDir dist/server --ssr src/entry.server.tsx",
    "start": "NODE_ENV=production node server/server.js",
    "preview": "vite preview --open",
    "preview-build": "tsc && vite build && vite preview",
    "deploy": "firebase deploy --token \"$FIREBASE_TOKEN\" --only hosting",
    "test": "vitest run",
    "test-watch": "vitest"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase": "^10.1.0",
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^4.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-scroll": "^1.9.0",
    "sirv": "^2.0.3",
    "styled-components": "^6.0.8",
    "uuid": "^9.0.0",
    "zustand": "^4.4.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/compression": "^1.7.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/react-scroll": "^1.8.10",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.5.1",
    "vitest": "^0.34.6"
  }
}
